<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Extend Material Global and Local for Nav(PA01) to JDE(SG04)">
	<test name="HeiPort_Login_LDR">
