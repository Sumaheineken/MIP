			<class name="com.mendix.test.LoginScript">
				<methods>
					<include name="tasKill" />
					<include name="openBrowser" />
					<include name="loginAsLDR" />
				</methods>
			</class>
		</classes>
	</test>
	<test name="Material_Extend_Process">
